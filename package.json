{
  "name": "compile-with-whitespace",
  "version": "1.0.0",
  "description": "A tool that allows you to compile TypeScript to JavaScript while maintaining whitespace",
  "bin": {
    "compileTSWithWhitespace": "./dist/index.cjs"
  },
  "repository": "https://github.com/aryaemami59/compile-with-whitespace",
  "homepage": "https://github.com/aryaemami59/compile-with-whitespace#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "test": "ts-node src/compileExamples.ts examples",
    "build": "tsup",
    "start": "npx compileTSWithWhitespace examples tsconfig.json",
    "clean": "rimraf -g \"examples/**/*.js\" \"examples/dist\""
  },
  "keywords": [
    "compile",
    "whitespace",
    "typescript",
    "javascript",
    "ts",
    "js"
  ],
  "author": "Arya Emami <aryaemami59@yahoo.com> (https://github.com/aryaemami59)",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.1.1",
    "reselect": "^5.0.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "typescript": "^5.3.3"
  }
}
